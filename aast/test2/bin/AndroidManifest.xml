<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="idc.aast.test2"
    android:versionCode="34"
    android:versionName="3.3" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS" />
    <uses-permission android:name="com.sec.android.provider.badge.permission.READ" />
    <uses-permission android:name="com.sec.android.provider.badge.permission.WRITE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <!--
  IMPORTANT: Change "com.parse.tutorials.pushnotifications.permission.C2D_MESSAGE" in the lines below
  to match your app's package name + ".permission.C2D_MESSAGE".
    -->
    <permission
        android:name="idc.aast.test2.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="idc.aast.test2.permission.C2D_MESSAGE" />

    <application
        android:name="idc.aast.Other.application"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher3"
        android:label="@string/app_name"
        android:theme="@style/Theme.Example" >
        
        <activity
            android:name="idc.aast.edu.activities.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name="idc.aast.Other.IntentReciever"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.SCREEN_OFF" />
                <action android:name="android.intent.action.SCREEN_ON" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>

        <activity
            android:name="idc.aast.edu.activities.ListActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_list"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Example" >
            <meta-data
                android:name="android.support.UI_OPTIONS"
                android:value="splitActionBarWhenNarrow" />
        </activity>
        <activity
            android:name="idc.aast.edu.activities.Accounts"
            android:configChanges="orientation"
            android:label="@string/title_activity_accounts"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.Morasalat"
            android:configChanges="orientation"
            android:label="@string/title_activity_morasalat"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.Results"
            android:configChanges="orientation"
            android:label="@string/title_activity_results"
            android:screenOrientation="portrait" >
        </activity>

        <service android:name="com.parse.PushService" />

        <receiver android:name="com.parse.ParseBroadcastReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.parse.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <!-- IMPORTANT: Change "com.parse.tutorials.pushnotifications" to match your app's package name. -->
                <category android:name="idc.aast.test2" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.parse.ParsePushBroadcastReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.parse.push.intent.RECEIVE" />
                <action android:name="com.parse.push.intent.DELETE" />
                <action android:name="com.parse.push.intent.OPEN" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="idc.aast.Other.ParseCustomReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="idc.aast.test2.UPDATE_STATUS" />
            </intent-filter>
        </receiver>

        <activity
            android:name="idc.aast.edu.activities.SchedueleDetails"
            android:configChanges="orientation"
            android:label="@string/title_activity_scheduele_details"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.CourseDetails"
            android:configChanges="orientation"
            android:label="@string/title_activity_course_details"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.NewsDetails"
            android:configChanges="orientation"
            android:label="@string/title_activity_news_details"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.TabMain"
            android:configChanges="orientation"
            android:label="@string/title_activity_tab_main"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.fragments.NotificationFragment"
            android:configChanges="orientation"
            android:label="@string/title_activity_notification_fragment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.fragments.GamesFragment"
            android:configChanges="orientation"
            android:label="@string/title_activity_games_fragment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.activities.Community"
            android:configChanges="orientation"
            android:label="@string/title_activity_community"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.test2.Ticketing"
            android:configChanges="orientation"
            android:label="@string/title_activity_ticketing"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.edu.fragments.GroupPostFragment"
            android:configChanges="orientation"
            android:label="@string/title_activity_group_post_fragment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.test2.HomePage"
            android:configChanges="orientation"
            android:label="@string/title_activity_home_page"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.test2.Test"
            android:configChanges="orientation"
            android:label="@string/title_activity_test"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.test2.LinksFragment"
            android:configChanges="orientation"
            android:label="@string/title_activity_links_fragment"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="idc.aast.test2.EmailinboxActivity"
            android:label="@string/title_activity_emailinbox" >
        </activity>
    </application>

</manifest>